var lastJob = null;
// **************************************************************************************
function IsInstalled(dir, uri){
    // core
    if(uri.endsWith("/core")) 
        return true;        
    
    var res = false;
	lastJob = new THTTPJob();
	lastJob.verb = "GET";
	lastJob.url  = dir.url;
    lastJob.uri  = uri;
    
	lastJob.execute();
    
    if (!lastJob.wasError) {
    	//trace(uri);
    	//trace(lastJob.responseStatus);
        if (lastJob.responseStatus == 301 || lastJob.responseStatus == 302)
            {
                var locationStr = lastJob.response.headerValue('location');
    			if (locationStr && locationStr.indexOf(uri + "/") != -1)  
                    res = true;                
            }            
    }    
	return res;     
}
// **************************************************************************************
function addToCrawler(root_path, plugin_dir, getData, postData) {
    // add request
    var r = new THTTPRequest();
    var dir = getCurrentDirectory();
    // if the URL starts with # we start from wordpress root directory
    if (getData.indexOf("#") == 0) {
        var uridata = getData.slice(1);                            
        r.URI = root_path + uridata;                            
    } else r.URI = plugin_dir + getData;
    if (postData) {                            
        r.Verb = 'POST';
        r.body = postData;
        r.addHeader('Content-Type', 'application/x-www-form-urlencoded', true);
    } 
    else r.Verb = 'GET';
    // audit other wordpress stuffs 
    // uploadify
    if (r.URI.endsWith("uploadify.php") || r.URI.endsWith("custom.php")){
        testUploadifyOnURI(r.URI);
    }    
	r.addHeader('Host', scanUrl.hostPort, true);
    // send it to the crawler
    //trace(r.toString());
    addHTTPRequestToCrawler(r);    
}
// **************************************************************************************
function detectInstalledPluginsAndThemes(dir, root_path) {
    // get list of plugins 
    for (var i=0; i<files.length; i++)
        if (files[i][0] == 'p' && files[i][1])
    	    plugins.push(files[i][1]);
    // remove duplicates
    plugins = eliminateDuplicates(plugins);
    
    // get list of themes 
    for (var i=0; i<files.length; i++)
        if (files[i][0] == 't' && files[i][1])
    	    themes.push(files[i][1]);
    // remove duplicates
    themes = eliminateDuplicates(themes);
    // look for false positives in plugins
    var plugin_dir = root_path + "wp-content/plugins/" + randStr(8);
    var bogusDir = false;
    if (IsInstalled(dir, plugin_dir)) {
        bogusDir = true;
    }
    
    // check plugins
    if (!bogusDir) {
        for (var i=0; i<plugins.length; i++){
            var plugin_dir = root_path + "wp-content/plugins/" + plugins[i];
            if (IsInstalled(dir, plugin_dir)) {
                //trace("plugin installed " + plugins[i]);
                
                // add http requests to the crawler
                for (var j=0; j<files.length; j++)
                	if (files[j][0] == 'p' && files[j][1] == plugins[i]) {
                		
                        var getData = files[j][2];
                        var postData = files[j][3];
                        
                        addToCrawler(root_path, plugin_dir, getData, postData);
                }
            }           
        }
    }
    
    // look for false positives in themes
    var theme_dir = root_path + "wp-content/themes/" + randStr(8);
    var bogusDir = false;
    if (IsInstalled(dir, theme_dir)) {
        bogusDir = true;
    }
    
    // check themes
    if (!bogusDir) {
        for (var i=0; i<themes.length; i++){
            var theme_dir = root_path + "wp-content/themes/" + themes[i];
            if (IsInstalled(dir, theme_dir)) {
                //trace("theme installed " + themes[i]);
                
                // add http requests to the crawler
                for (var j=0; j<files.length; j++)
                	if (files[j][0] == 't' && files[j][1] == themes[i]) {
                        var getData = files[j][2];
                        var postData = files[j][3];
                        
                        addToCrawler(root_path, theme_dir, getData, postData);
                }
            }           
        }
    }    
}
// **************************************************************************************
function testForVulnPluginsAndThemes(dir) {
    root_path = dir.fullPath;
    if (root_path.charAt(root_path.length-1) != '/') root_path = root_path + '/';
    
    detectInstalledPluginsAndThemes(dir, root_path);
}
